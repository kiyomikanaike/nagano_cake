<div class="col-5 ml-3">
  <h4>配送先登録/一覧</h4>
  <%= form_with model: Address.new, url: address_index_path do |f| %>
  <table>
  <tr>
    <th>郵便番号（ハイフンなし）</th>
    <td><%= f.text_field:address_postal_code %></td>
  </tr>
  <tr>
    <th>住所</th>
    <td><%= f.text_area :address_address %></td>
  </tr>
  <tr>
    <th>宛名</th>
    <td><%= f.text_field :address_name %></td>
  </tr>
  </table>
  <%= f.submit "新規登録",class: 'btn btn-success' %>
  <% end %>
</div>